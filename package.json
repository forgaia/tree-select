{
  "name": "gaia-patient-models",
  "version": "1.1.0",
  "private": true,
  "author": "HCL Technologies",
  "scripts": {
    "generate-cert": "mkcert -cert-file localhost.pem -key-file localhost-key.pem localhost 127.0.0.1 ::1 ",
    "analyze": "source-map-explorer \"build/static/js/*.js\"",
    "eject": "react-scripts eject",
    "build": "react-app-rewired build",
    "start": "react-app-rewired start",
    "test": "cross-env CI=true react-scripts test --runInBand",
    "coverage": "react-scripts test --watchAll --coverage --runInBand",
    "lint": "cross-env TIMING=1 eslint -c ./.eslintrc './src/**/*.{ts,tsx}'",
    "lint:fix": "cross-env TIMING=1 eslint -c ./.eslintrc --fix './src/**/*.{ts,tsx}'",
    "cypress": "cypress open --browser chrome",
    "cypress:headless": "cypress run --headless --browser chrome",
    "e2e": "concurrently 'cross-env REACT_APP_ENV=test npm start' 'npm run cypress'",
    "e2e:headless": "concurrently 'cross-env REACT_APP_ENV=test npm start' 'npm run cypress:headless'",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "preinstall": "node preinstall-script.js"
  },
  "engines": {
    "node": ">=10.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint -c ./.eslintrc --fix"
    ]
  },
  "browserslist": [
    "chrome >=55",
    "edge >=17"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.styles.{ts,tsx}",
      "!src/**/*.style.{ts,tsx}",
      "!src/**/*.types.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!src/**/*.story.{ts,tsx}",
      "!src/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "src/assets",
      "src/mocks"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "4.0.2",
    "@forgaia/fetch": "1.0.13",
    "@forgaia/value-formatter": "1.0.0",
    "@sentry/browser": "5.13.0",
    "@storybook/addon-actions": "5.3.14",
    "@storybook/addon-docs": "5.3.14",
    "@storybook/addon-knobs": "5.3.14",
    "@storybook/addon-links": "5.3.14",
    "@storybook/addon-storysource": "5.3.14",
    "@storybook/addons": "5.3.14",
    "@storybook/preset-create-react-app": "1.5.2",
    "@storybook/react": "5.3.14",
    "@types/adal-angular": "1.0.1",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/history": "4.7.5",
    "@types/jest": "25.1.3",
    "@types/lodash": "4.14.149",
    "@types/node": "13.7.7",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "7.1.7",
    "@types/react-router": "5.1.4",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "16.9.2",
    "@types/redux-mock-store": "1.0.2",
    "@types/styled-components": "5.0.1",
    "adal-angular": "1.0.17",
    "antd": "4.0.2",
    "connected-react-router": "6.7.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "history": "4.10.1",
    "lodash": "4.17.15",
    "polished": "3.4.4",
    "rc-field-form": "1.1.0",
    "react": "16.13.0",
    "react-app-polyfill": "1.0.6",
    "react-dom": "16.13.0",
    "react-redux": "7.2.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-test-renderer": "16.13.0",
    "react-use": "13.26.4",
    "redux": "4.0.5",
    "redux-mock-store": "1.5.4",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "register-service-worker": "1.6.2",
    "reselect": "4.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "typescript": "3.8.3",
    "react-scripts": "3.4.0",
    "@babel/runtime": "7.8.4",
    "@commitlint/cli": "8.3.5",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "babel-plugin-import": "1.13.0",
    "babel-plugin-polished": "1.1.0",
    "babel-plugin-styled-components": "1.10.7",
    "concurrently": "5.1.0",
    "cross-env": "7.0.0",
    "customize-cra": "0.9.1",
    "cypress": "4.1.0",
    "enzyme-to-json": "3.4.4",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-airbnb-typescript": "7.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "husky": "4.2.3",
    "jest-styled-components": "7.0.0",
    "less": "3.11.1",
    "less-loader": "5.0.0",
    "lint-staged": "10.0.8",
    "node-sass": "4.13.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "react-app-rewired": "2.1.5",
    "sass-loader": "8.0.2",
    "source-map-explorer": "2.3.1",
    "ts-loader": "6.2.1"
  }
}
